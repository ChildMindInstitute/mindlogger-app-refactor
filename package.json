{
  "name": "mindlogger-mobile",
  "version": "0.22.0",
  "private": true,
  "scripts": {
    "android": "yarn android:dev",
    "ios": "yarn ios:dev",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pods": "cd ios && pod install",
    "prepare": "husky install",
    "postinstall": "react-native setup-ios-permissions",
    "start:reset-cache": "watchman watch-del-all && react-native start --reset-cache",
    "android:dev": "react-native run-android --variant=devdebug --appIdSuffix=dev",
    "android:dev-release": "react-native run-android --variant=devrelease --appIdSuffix=dev",
    "android:qa": "react-native run-android --variant=qadebug --appIdSuffix=qa",
    "android:qa-release": "react-native run-android --variant=qarelease --appIdSuffix=qa",
    "android:staging": "react-native run-android --variant=stagingdebug --appIdSuffix=staging",
    "android:staging-release": "react-native run-android --variant=stagingrelease --appIdSuffix=staging",
    "android:production": "react-native run-android --variant=productiondebug",
    "android:production-release": "react-native run-android --variant=productionrelease",
    "ios:dev": "react-native run-ios --scheme MindloggerMobileDev",
    "ios:dev-release": "react-native run-ios --scheme MindloggerMobileDev --configuration Release",
    "ios:qa": "react-native run-ios --scheme MindloggerMobileQA",
    "ios:qa-release": "react-native run-ios --scheme MindloggerMobileQA --configuration Release",
    "ios:staging": "react-native run-ios --scheme MindloggerMobileStaging",
    "ios:staging-release": "react-native run-ios --scheme MindloggerMobileStaging --configuration Release",
    "ios:production": "react-native run-ios --scheme MindloggerMobile",
    "ios:production-release": "react-native run-ios --scheme MindloggerMobile --configuration Release"
  },
  "dependencies": {
    "@georstat/react-native-image-cache": "^2.3.0",
    "@hookform/resolvers": "^2.9.10",
    "@miblanchard/react-native-slider": "^2.3.1",
    "@notifee/react-native": "^7.6.1",
    "@react-native-community/checkbox": "^0.5.14",
    "@react-native-community/datetimepicker": "^6.7.5",
    "@react-native-community/geolocation": "^3.0.5",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/native": "6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@reduxjs/toolkit": "^1.9.1",
    "@shopify/react-native-skia": "^0.1.182",
    "@tamagui/animations-react-native": "^1.11.1",
    "@tamagui/button": "^1.11.1",
    "@tamagui/core": "^1.11.1",
    "@tamagui/focusable": "^1.11.1",
    "@tamagui/image": "^1.11.1",
    "@tamagui/linear-gradient": "^1.11.1",
    "@tamagui/portal": "^1.11.1",
    "@tamagui/radio-group": "^1.11.1",
    "@tamagui/shorthands": "^1.11.1",
    "@tamagui/stacks": "^1.11.1",
    "@tamagui/text": "^1.11.1",
    "@tamagui/theme-base": "^1.11.1",
    "@tanstack/query-async-storage-persister": "^4.20.9",
    "@tanstack/react-query": "^4.20.9",
    "@tanstack/react-query-persist-client": "^4.20.9",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "dayspan": "^1.1.0",
    "eventemitter3": "^5.0.0",
    "i18next": "^22.4.6",
    "markdown-it-container": "^3.0.0",
    "react": "18.1.0",
    "react-hook-form": "^7.41.0",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.6",
    "react-native-audio-recorder-player": "^3.5.3",
    "react-native-autoheight-webview": "^1.6.5",
    "react-native-background-fetch": "^4.1.9",
    "react-native-config": "^1.4.12",
    "react-native-file-access": "^2.5.2",
    "react-native-get-random-values": "^1.8.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-markdown-display": "6.1.6",
    "react-native-mime-types": "^2.3.0",
    "react-native-mmkv": "2.5.1",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-permissions": "^3.7.3",
    "react-native-popover-view": "^5.1.7",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "^3.20.0",
    "react-native-select-dropdown": "^3.3.2",
    "react-native-svg": "^13.8.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-video-player": "^0.14.0",
    "react-native-webview": "^11.26.1",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "rn-fetch-blob": "^0.12.0",
    "use-debounce": "^9.0.4",
    "uuid": "^9.0.0",
    "victory-native": "^36.6.8",
    "zod": "^3.20.2",
    "zod-i18n-map": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/markdown-it-container": "^2.0.5",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-native-video": "^5.0.14",
    "@types/react-native-video-player": "^0.10.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-flipper": "0.163.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "18.1.0",
    "redux-flipper": "^2.0.2",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "react": "18.1.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "bash -c tsc --noEmit --skipLibCheck",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "reactNativePermissionsIOS": [
    "LocationWhenInUse",
    "Camera",
    "Microphone",
    "PhotoLibrary"
  ]
}
